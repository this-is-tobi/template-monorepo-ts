{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:clean": {
      "cache": false
    },
    "compile": {
      "outputs": ["types/**"]
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "format:style": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:cov": {
      "outputs": ["coverage/**"]
    },
    "test:ct": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "^@template-monorepo-ts/test-utils#build"
      ],
      "outputs": []
    },
    "test:ct-ci": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "^@template-monorepo-ts/test-utils#build"
      ],
      "outputs": []
    },
    "@template-monorepo-ts/cypress#test:e2e-ci": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "@template-monorepo-ts/test-utils#build",
        "@template-monorepo-ts/docs#test:e2e-ci",
        "@template-monorepo-ts/api#test:e2e-ci"
      ],
      "inputs": ["src/**"],
      "outputs": []
    },
    "test:e2e-ci": {
      "inputs": ["src/**"],
      "outputs": []
    }
  }
}