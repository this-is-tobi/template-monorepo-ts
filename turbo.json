{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:clean": {
      "cache": false
    },
    "compile": {
      "outputs": ["types/**"]
    },
    "dev": {
      "cache": false
    },
    "//#format:root": {
      "outputs": []
    },
    "format": {
      "dependsOn": ["//#format:root"],
      "outputs": []
    },
    "//#lint:root": {
      "outputs": []
    },
    "lint": {
      "dependsOn": ["//#lint:root"],
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:ui": {
      "outputs": []
    },
    "test:cov": {
      "outputs": ["coverage/**"]
    },
    "test:ct": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "^@template-monorepo-ts/test-utils#build"
      ],
      "outputs": []
    },
    "test:ct:ui": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "^@template-monorepo-ts/test-utils#build"
      ],
      "outputs": []
    },
    "test:e2e": {
      "inputs": ["src/**"],
      "outputs": []
    },
    "test:e2e:ui": {
      "inputs": ["src/**"],
      "outputs": []
    },
    "@template-monorepo-ts/playwright#test:e2e": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "@template-monorepo-ts/test-utils#build",
        "@template-monorepo-ts/docs#test:e2e",
        "@template-monorepo-ts/api#test:e2e"
      ],
      "inputs": ["src/e2e/**"],
      "outputs": []
    },
    "@template-monorepo-ts/playwright#test:e2e:ui": {
      "dependsOn": [
        "^@template-monorepo-ts/shared#build",
        "@template-monorepo-ts/test-utils#build",
        "@template-monorepo-ts/docs#test:e2e",
        "@template-monorepo-ts/api#test:e2e"
      ],
      "inputs": ["src/e2e/**"],
      "outputs": []
    }
  }
}
