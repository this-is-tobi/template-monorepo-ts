{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:clean": {},
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "format:style": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:cov": {
      "outputs": []
    },
    "test:ct": {
      "dependsOn": [
        "^@template-monorepo/test-utils#build"
      ],
      "outputs": []
    },
    "test:ct-ci": {
      "dependsOn": [
        "^@template-monorepo/test-utils#build"
      ],
      "outputs": []
    },
    "@template-monorepo/cypress#test:e2e-ci": {
      "dependsOn": [
        "@template-monorepo/test-utils#build",
        "docs#test:e2e-ci",
        "api#test:e2e-ci"
      ],
      "inputs": [
        "src/**"
      ],
      "outputs": []
    },
    "test:e2e-ci": {
      "inputs": [
        "src/**"
      ],
      "outputs": []
    }
  }
}